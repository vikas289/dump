<!DOCTYPE html>
<!--[if lt IE 9 ]><html class="no-js oldie" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="no-js oldie ie9" lang="en"> <![endif]-->
<!--[if (gte IE 9)|!(IE)]><!-->
<html class="no-js" lang="en">
<!--<![endif]-->

<head>

    <!--- basic page needs
    ================================================== -->
    <meta charset="utf-8">
    <title>@ViewBag.Title - IPCA</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- mobile specific metas
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- CSS
    ================================================== -->
    <link rel="stylesheet" href="~/Content/css/base.css">
    <link rel="stylesheet" href="~/Content/css/vendor.css">
    <link rel="stylesheet" href="~/Content/css/main.css">
    @*@Styles.Render("~/Content/css")*@
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/css/jquery.steps.css">
    <link href="~/Content/css/jquery-nicelabel.css" rel="stylesheet" />

    <!-- script
    ================================================== -->
    <script src="~/Content/js/modernizr.js"></script>
    <script src="~/Content/js/pace.min.js"></script>

    @*@Scripts.Render("~/bundles/modernizr")*@
    <!-- favicons
    ================================================== -->
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <link rel="icon" href="favicon.ico" type="image/x-icon">

</head>

<body id="top">

    <!-- header
    ================================================== -->
    <header id="header" class="row">

        <div class="header-logo">
            <a href="#">IPCA</a>
        </div>

        <nav id="header-nav-wrap">
            <ul class="header-main-nav">
                <li><a class="smoothscroll" href="#quiz" title="Take Quiz">Take Quiz</a></li>
            </ul>
        </nav>

        <a class="header-menu-toggle" href="#"><span>Menu</span></a>

    </header> <!-- /header -->


    @RenderBody()


    <footer>



        <div class="footer-bottom">

            <div class="row">

                <div class="col-twelve">
                    <div class="copyright">
                        <span>© Copyright IPCA 2021.</span>
                        <span>Design by <a href="#">Exicon</a></span>
                    </div>

                    <div id="go-top">
                        <a class="smoothscroll" title="Back to Top" href="#top"><i class="icon-arrow-up"></i></a>
                    </div>
                </div>

            </div> <!-- end footer-bottom -->

        </div>

    </footer>

    <div id="preloader">
        <div id="loader"></div>
    </div>

    <!-- Java Script
    ================================================== -->
    <script src="~/Content/js/jquery-2.1.3.min.js"></script>
    @*@Scripts.Render("~/bundles/bootstrap")*@
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Content/js/plugins.js"></script>

    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/0.4.1/html2canvas.min.js"></script>
    <script src="~/Content/js/main.js"></script>
    <script src="~/Content/js/jquery.steps.js"></script>
    <script src="~/Content/js/jquery.nicelabel.js"></script>
    @RenderSection("scripts", required: false)
</body>

</html>

@*@Scripts.Render("~/bundles/jquery")*@
